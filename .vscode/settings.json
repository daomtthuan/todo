{
  "cSpell.words": ["daomtthuan", "dotenvx", "mongodb", "tsdoc", "bson"],
  "files.exclude": {
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.svn": true,
    "**/CVS": true,
    "**/Thumbs.db": true
  },
  "files.readonlyInclude": {
    "**/node_modules/**/*": true,
    "**/package-lock.json": true
  },
  "formatFiles.excludePattern": "**/node_modules/**,**/node_modules/**/*,**/node_modules/.**/*,**/node_modules/**/.*,*-lock.json,**/*-lock.json",
  "search.exclude": {
    "**/*.code-search": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/package-lock.json": true
  },
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.ts": "${capture}.js",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "tsconfig.json": "tsconfig.*.json",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb"
  }
}
